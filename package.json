{
  "name": "spylink-pro",
  "version": "3.0.0",
  "description": "Ultimate Telegram-based tracking link generator with extreme fingerprinting, real-time data exfiltration, PDF reports & admin dashboard.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm ci --omit=dev && echo 'Production build ready'",
    "test": "echo \"No tests implemented yet\" && exit 0",
    "generate:fonts": "mkdir -p fonts && cp node_modules/pdfmake/examples/fonts/* fonts/ 2>/dev/null || echo 'Fonts not found, using fallback'",
    "postinstall": "npm run generate:fonts"
  },
  "keywords": [
    "telegram-bot",
    "tracking-link",
    "fingerprinting",
    "data-collection",
    "pdf-report",
    "admin-panel",
    "cloudflare-mimic",
    "webview-spy",
    "camera-capture",
    "location-tracking"
  ],
  "author": "aadi_io",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aadi-io/spylink-pro.git"
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "node-fetch": "^2.7.0",
    "dotenv": "^16.4.5",
    "node-telegram-bot-api": "^0.64.0",
    "ejs": "^3.1.10",
    "pdfmake": "^0.2.7",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
